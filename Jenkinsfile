pipeline {
    agent any
   
       
    stages {
       
          stage ('compile') {
            steps {
                sh "iam compiling" 
            }
         }
          stage ('test') {
            steps {
                sh "i am testing" 
            }
         }
          stage ('package') {
            steps {
                sh "i am packaging" 
            }
         }
          stage ('Build') {
            steps {
                sh "i am building" 
            }
         }
    
    }
}
